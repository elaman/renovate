{
    "name": "systemseed/globethics",
    "description": "Project template for Drupal projects.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://systemseed.com",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "composer/installers": "^2.1",
        "cweagans/composer-patches": "^2",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3.1",
        "drupal/advanced_email_validation": "^2.0",
        "drupal/advban": "^1.8",
        "drupal/allowed_formats": "^3.0",
        "drupal/autoban": "^1.11",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/color_field": "^3.0",
        "drupal/colorbox": "^2.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_filter": "^2.4",
        "drupal/config_pages": "^2.15",
        "drupal/core-composer-scaffold": "^11",
        "drupal/core-recommended": "^11",
        "drupal/cshs": "^4.0",
        "drupal/daterange_compact": "^2.0",
        "drupal/display_link_plus": "^1.1",
        "drupal/doi_field": "^2",
        "drupal/domino": "^4.0",
        "drupal/drupalauth4ssp": "^2.0",
        "drupal/email_registration": "^2",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/eu_cookie_compliance": "^1.28",
        "drupal/extlink": "^2.0",
        "drupal/fast_404": "^3.4",
        "drupal/field_config_cardinality": "^4.0",
        "drupal/field_group": "^4.0",
        "drupal/focal_point": "^2.0",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "^2.1",
        "drupal/hotjar": "^4.0",
        "drupal/imageapi_optimize": "^4.1",
        "drupal/imageapi_optimize_gd": "2.x-dev@dev",
        "drupal/isbn": "^1.4",
        "drupal/jotform_field_widget": "^1.0@RC",
        "drupal/json_field": "^1.3",
        "drupal/length_indicator": "^1.2",
        "drupal/linkit": "^7.0",
        "drupal/material_base": "^3.0",
        "drupal/menu_admin_per_menu": "^1.5",
        "drupal/menu_block": "^1.10",
        "drupal/menu_item_extras": "^3.0",
        "drupal/metatag": "^2.0",
        "drupal/mimemail": "2.0.x-dev@dev",
        "drupal/module_filter": "^5.0",
        "drupal/moodle_field_widget": "^1.0@RC",
        "drupal/no_nbsp": "^1.5",
        "drupal/nomarkup": "^1.0",
        "drupal/optional_end_date": "^1.3",
        "drupal/override_node_options": "^2.6",
        "drupal/paragraphs": "^1.16",
        "drupal/paragraphs_browser": "^1.1",
        "drupal/paragraphs_previewer": "^1.7",
        "drupal/pathauto": "^1.12",
        "drupal/prevent_user_delete_reassign": "^1.0",
        "drupal/rabbit_hole": "^2.0@alpha",
        "drupal/real_aes": "^2.5",
        "drupal/realname": "^2.0@beta",
        "drupal/recaptcha": "^3.4",
        "drupal/recaptcha_v3": "^2.0",
        "drupal/redirect": "^1.9",
        "drupal/reroute_email": "^2.3",
        "drupal/reverse_proxy_header": "^1.1",
        "drupal/role_delegation": "^1.2",
        "drupal/rollbar": "^2.1",
        "drupal/select2": "^2.0",
        "drupal/simple_activecampaign": "^1.0",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smtp": "^1.0",
        "drupal/tfa": "^1.4",
        "drupal/time_field": "^2.1",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/user_registrationpassword": "^2",
        "drupal/video_embed_field": "^3.0",
        "drupal/views_data_export": "^1.4",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/webform": "^6.3@beta",
        "drupal/yearonly": "^9.1",
        "drush/drush": "^13",
        "jotform/jotform-api-php": "dev-master",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "simplesamlphp/simplesamlphp": "^2.3",
        "systemseed/simplesamlphp-module-rollbar": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^11",
        "mglaman/phpstan-drupal": "^2.0",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "1.1.0",
        "drupal/fast_404": "3.5",
        "drupal/eu_cookie_compliance": "1.29"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "phpstan/extension-installer": true,
            "drupal/console-extend-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "simplesamlphp/composer-module-installer": true,
            "tbachert/spi": true,
            "simplesamlphp/composer-xmlprovider-installer": true
        },
        "platform": {
            "php": "8.3"
        }
    },
    "extra": {
        "installer-types": ["bower-asset", "npm-asset"],
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patches-file": "./patches.json",
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/.csslintrc": false,
                "[web-root]/.eslintignore": false,
                "[web-root]/.eslintrc.json": false,
                "[web-root]/.ht.router.php": false,
                "[web-root]/.htaccess": false,
                "[web-root]/robots.txt": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/sites/example.settings.local.php": false,
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/sites/default/default.services.yml": false,
                "[web-root]/sites/default/default.settings.php": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.txt": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
